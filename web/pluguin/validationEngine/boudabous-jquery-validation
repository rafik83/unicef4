<script>
    $(function () {
        var o = 0;
        setInterval(function () {
            if (o == 0) {
                $('#matTitle').css('color', 'red');
                o = 1;
            } else {
                $('#matTitle').css('color', 'black');
                o = 0;
            }
        }, 1000);
        $('.fos_user_registration_register select').each(function () {
            $(this).select2();

        });
        $('.s2id_fos_user_registration_form_roles_0').each(function () {
            $(this).attr('style', 'width:200px');
        });
        $(".scroll").mCustomScrollbar();
        $('#id_employe_search').select2();
        $('#id_employe_search').select2('val','');
        $('#go').click(function(){
            var idEmploye = $('#id_employe_search').val();
            var url = '/employe/valIdEmploye/update';
            url= url.replace('valIdEmploye', idEmploye);
            window.open(url);
        });
        $('#go').hide();
        $('#id_employe_search').change(function(){
            if($(this).val()!=''){
                $('#go').show();
            }else{
                $('#go').hide();
            }
        });
    })

</script>
