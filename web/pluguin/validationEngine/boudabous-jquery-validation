    <script>
        $(function () {
            $("#form_societe").validationEngine();
            $("#date_creation").datepicker();

            $("#save_btn_societe").click({action: "new"}, saveAction);
            $("#save_and_list_btn_societe").click({action: "list"}, saveAction);
            $('#menu_compte_bancaire').click(function () {
                if ($("#div_compte_bancaire").html() == '') {
                    $("#image_loading").show();
                    $.ajax({
                        url: '/societe/societe_update_compte_bancaire',
                        success: function (data) {
                            $("#div_compte_bancaire").html(data);
                            $("#div_compte_bancaire").show();
                            $("#image_loading").hide();
                        }
                    });
                }
            });
            $('#menu_adresse').click(function () {
                if ($("#div_adresse").html() == '') {
                    $("#image_loading").show();
                    $.ajax({
                        url: '/societe/societe_update_adresse',
                        success: function (data) {
                            $("#div_adresse").html(data);
                            $("#div_adresse").show();
                            $("#image_loading").hide();
                        }
                    });
                }
            });
            $('#menu_autre').click(function () {
                if ($("#div_autre").html() == '') {
                    $("#image_loading").show();
                    $.ajax({
                        url: '/societe/societe_update_autre',
                        success: function (data) {
                            $("#div_autre").html(data);
                            $("#div_autre").show();
                            $("#image_loading").hide();
                        }
                    });
                }
            });
            $(".checker").show();
            $(".radio").show();
            $(".uploader").show();
        });
        function verifierAdresse() {
            var reseult = true;
            if ($("#id_pays").val() != "") {
                if ($("#id_ville").val() == "" || $("#id_code_postale").val() == "") {
                    reseult = false;
                }
            }
            return reseult;
        }
        function saveAction(event) {
            $('#myTab').tabs({ selected: 0 });
//            $("#form_societe input,select").each(function () {
//                $(this).removeClass();
//            });
            if ($("#form_societe").validationEngine('validate') && verifierAdresse()) {
                var selector = $('#selector').val();
                formDataAll = $(selector).toObject({mode: 'all'}),
                        alert(JSON.stringify(formDataAll, null, '\t'));
                $("#image_loading").show();
                $.ajax({
                    type: 'Post',
                    url: "/societe/societe_update_societe",
                    data: 'societe=' + JSON.stringify(formDataAll, null, '\t'),
                    success: function (data) {
                        $("#image_loading").hide();
                        $(".workplace").html(data);
                        $(".tabs").tabs();
                    }
                });
                var image = $('#image')[0].files[0];
                formdata = new FormData();
                formdata.append( 'image', image );
                if($('#image').val()!='')
                    $.ajax({
                        cache: false,
                        contentType: false,
                        processData: false,
                        type: 'Post',
                        url: "/societe/societe_save_logo",
                        data: formdata,
                        success: function (data) {
                            window.location.reload();
                        }
                    });
            } else {
                if (!verifierAdresse()) {
                    alert("verifié l'adresse");
                }
            }
        }
    </script>
        </div>

    </div>
