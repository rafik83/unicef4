<script>

    function readURL(input) {
        var ext = $('#image').val().split('.').pop().toLowerCase();
        if ($.inArray(ext, ['gif', 'png', 'jpg', 'jpeg']) == -1) {
            alert('invalid extension!');
            return resetFile(input);
        }
        if (input.files[0].size > 1048576) {
            alert("File should be less than 1Mb");
            return resetFile(input);
        }
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            reader.onload = function (e) {
                $('#image1').attr('src', e.target.result);
            };
            reader.readAsDataURL(input.files[0]);
        }
    }


    $(function () {
        $('#image').uniform();
        $('.image-popup-vertical-fit').magnificPopup({
            type: 'image',
            closeOnContentClick: true,
            mainClass: 'mfp-img-mobile',
            image: {
                verticalFit: true
            }
        });
        $("#nombre_paie").spinner({
            scale: 0,
            width: 50,
            val: '12'
        });
        $("select").each(function () {
            $(this).select2();
            $(this).select2("val", "");
        });
        $("#id_nationalite").select2("val", '112');
        $("#id_convention").select2("val", '');
        $("#id_regime").select2("val", '152');
        $("#id_devise").select2("val", '75');
        $("#id_assurance").select2("val", '113');
        $("#id_categorie_comptable").select2("val", '76');
        $("#id_forme").select2("val", '150');
        $("#id_taxe").select2("val", '75');
        $("#id_nature").select2("val", '114');

    });
</script>

