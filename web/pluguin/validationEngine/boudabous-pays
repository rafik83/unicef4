    <script>
        //validation par ajax
        $('#confirm_btn').hide();
        pic1 = new Image(16, 16);
        pic1.src = "/img/loader.gif";
        $(function () {
            $("#silabe_paiebundle_pays_libelle").typing({
                stop: function () {
                    var usr = $("#silabe_paiebundle_pays_libelle").val();
                    if (usr.length >= 2) {
                        $("#status").html('<img src="/img/loader.gif" align="absmiddle">&nbsp;Vérification de la disponibilité...');
                        $.ajax({
                            type: "POST",
                            url: "/pays/pays_verification",
                            data: "libelle=" + usr,
                            success: function (msg) {
                                $("#status").ajaxComplete(function (event, request, settings) {
                                    //libelle autorisé
                                    if (msg == 'OK' && $("#silabe_paiebundle_pays_libelle").val().length >= 2) {
                                        if (!$("#cancel_btn").is(":visible"))
                                            $('#confirm_btn').show();
                                        $("#silabe_paiebundle_pays_libelle").removeClass('object_error'); // if necessary
                                        $("#silabe_paiebundle_pays_libelle").addClass("object_ok");
                                        $(this).html('&nbsp;<img src="/img/tick.gif" align="absmiddle">');
                                        $("#form_charge").validationEngine("validate");

                                    }
                                    //libelle non autorisé
                                    else if (!(msg == 'OK') && $("#silabe_paiebundle_pays_libelle").val().length >= 2) {
                                        $('#confirm_btn').hide();
                                        $("#silabe_paiebundle_pays_libelle").removeClass('object_ok'); // if necessary
                                        $("#silabe_paiebundle_pays_libelle").addClass("object_error");
                                        $(this).html(msg);
                                    }
                                });
                            }

                        });
                    }
                    else {
                        $('#confirm_btn').hide();
                        $('#form_charge').validationEngine('validate');
                        $('#status').html("");
                        $("#silabe_paiebundle_pays_libelle").removeClass('object_ok'); // if necessary
                        $("#silabe_paiebundle_pays_libelle").addClass("object_error");
                    }
                }});
        });
    </script>
    <script>
        var idPays = "";
        $(function () {
            $("#libelle_pays").val('');
            $("#cancel_btn").click(cancelAction);
            $("#update_btn").click({action: "update"}, crudAction);
            $("#confirm_btn").click({action: "create"}, crudAction);
        });
        function showPays(id, libelle) {
            $("#silabe_paiebundle_pays_libelle").val(libelle);
            $("#cancel_btn").show();
            $("#update_btn").show();
            $("#confirm_btn").hide();
            idPays = id;
        }
        function cancelAction() {
            $("#silabe_paiebundle_pays_libelle").val("");
            $("#cancel_btn").hide();
            $("#update_btn").hide();
            $("#status").html("");
        }
        function crudAction(e) {
            if ($("#form_charge").validationEngine("validate")) {
                $("#image_loading").show();
                $.ajax({
                    url: '/pays/list',
                    data: "action=" + e.data.action + "&libelle=" + $("#silabe_paiebundle_pays_libelle").val() + "&id=" + idPays + "&page=1",
                    success: function (data) {
                        $("#image_loading").hide();
                        $("#table_pays").html(data);
                        cancelAction();
                    }});
            }

        }
        function searchPays(page) {
            $("#image_loading").show();
            $.ajax({
                url: '/pays/list',
                data: "libelle_search=" + $("#libelle_pays").val() + "&page=" + page,
                success: function (data) {
                    $("#image_loading").hide();
                    $("#table_pays").html(data);
                    cancelAction();
                }});
        }

    </script>
        </div>

    </div>
    <div class="header" style='position:fixed;z-index: 10000; width:100%; height:20px; padding:5px; bottom:0px;text-align: center ;color: #ffffff'>
        copyright <a style="color: #ffffff" href="http://www.silabedev.com"> Silabe-Developpement</a> 2014
    </div>
    <div class="ajax_call" id="image_loading"></div>
</div>

</body>
